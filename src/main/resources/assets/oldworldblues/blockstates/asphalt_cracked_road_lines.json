{

    "multipart": [
        {   "when": { "variant": "0" },
            "apply": { "model": "oldworldblues:asphalt_cracked_0", "uvlock": true }
        },
        {   "when": { "variant": "1" },
            "apply": { "model": "oldworldblues:asphalt_cracked_1", "uvlock": true }
        },
        {   "when": { "variant": "2" },
            "apply": { "model": "oldworldblues:asphalt_cracked_2", "uvlock": true }
        },
        {   "when": { "variant": "3" },
            "apply": { "model": "oldworldblues:asphalt_cracked_3", "uvlock": true }
        },
        {   "when": { "variant": "4" },
            "apply": { "model": "oldworldblues:asphalt_cracked_4", "uvlock": true }
        },
        {   "when": { "line_axis": "x" },
            "apply": { "model": "oldworldblues:asphalt_road_lines", "y": 90, "uvlock": false }
        },
        {   "when": { "line_axis": "z" },
            "apply": { "model": "oldworldblues:asphalt_road_lines", "uvlock": false }
        }
    ]
}
