{
    "multipart": [
        {   "apply": { "model": "oldworldblues:vault_metal_plating" }},

        {   "when": { "up": "false" },
            "apply": { "model": "oldworldblues:vault_metal_plating_up", "uvlock": false }
        },
        {   "when": { "down": "false" },
            "apply": { "model": "oldworldblues:vault_metal_plating_down", "uvlock": false }
        },
        {   "when": { "east": "false" },
            "apply": { "model": "oldworldblues:vault_metal_plating_right", "y" : 0, "uvlock": false }
        },
        {   "when": { "west": "false" },
            "apply": { "model": "oldworldblues:vault_metal_plating_left", "y" : 0, "uvlock": false }
        },
        {   "when": { "north": "false" },
            "apply": { "model": "oldworldblues:vault_metal_plating_left", "y" : 90, "uvlock": false }
        },
        {   "when": { "south": "false" },
            "apply": { "model": "oldworldblues:vault_metal_plating_right", "y" : 90, "uvlock": false }
        }
    ]
}